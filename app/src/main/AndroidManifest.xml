<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.unesc.mastersysapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MasterSysAPP"
        android:usesCleartextTraffic="true">
        <activity android:name=".UserFormActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".ModalityFormActivity" />
        <activity android:name=".PlansFormActivity" />
        <activity android:name=".StudentFormActivity" />
        <activity android:name=".GraduationFormActivity" />
        <activity android:name=".MatriculationFormActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>